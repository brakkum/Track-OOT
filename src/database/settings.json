{
    "language": {
        "type": "choice",
        "default": "en_us",
        "values": "I18n.languages"
    },
    "font": {
        "type": "choice",
        "default": "font_default",
        "values": [
            "font_default",
            "font_charlemagne",
            "font_ravenna",
            "font_returnofganon",
            "font_zeldadx",
            "font_triforce",
            "font_sheikah"
        ]
    },
    "layout": {
        "type": "choice",
        "default": "map-compact",
        "values": [
            "items",
            "items-list",
            "map-right",
            "items-dt",
            "items-list-dt",
            "map-right-dt",
            "map-right-dtr",
            "map-center",
            "map-compact",
            "compact",
            "flat",
            "essential-map"
        ]
    },
    "item_size": {
        "type": "-range",
        "min": 32,
        "max": 64,
        "default": 40
    },
    "autosave_time": {
        "type": "number",
        "min": 0,
        "max": 120,
        "default": 10
    },
    "autosave_amount": {
        "type": "number",
        "min": 1,
        "max": 10,
        "default": 4
    },
    "unknown_dungeon_need_both": {
        "type": "check",
        "default": false
    },
    "use_custom_logic": {
        "type": "check",
        "default": false
    },
    "erase_all_data": {
        "type": "button",
        "text": "button_erase_data",
        "url": "uninstall.html?nosw",
        "target": "_self"
    }
}